{
    "compilerOptions": {
        "baseUrl": "src",
        "target": "es5",
        "lib": ["esnext"],
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "isolatedModules": true,
        "module": "esnext",
        "moduleResolution": "node",
        "noImplicitAny": false,
        "resolveJsonModule": true,
        "noEmit": true,
    },
    "include": ["src"]
}
